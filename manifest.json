{
    "manifest_version": 3,
    "name": "Calculator Extension",
    "version": "1.0",
    "description": "A simple calculator extension with customizable themes and history.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "storage"
    ]
  }
  